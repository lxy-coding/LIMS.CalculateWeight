      <generator class="uuid.hex">
        <param name="format">N</param>
      </generator>
    </id>
    <property name="STEEL_NUM">
      <column name="STEEL_NUM" sql-type="VARCHAR2" not-null="false" />
    </property>
    <property name="HEAT_NUM">
      <column name="HEAT_NUM" sql-type="VARCHAR2" not-null="false" />
    </property>
    <property name="POSITION_ID">
      <column name="POSITION_ID" sql-type="NUMBER" not-null="false" />
    </property>
    <property name="LAYER">
      <column name="LAYER" sql-type="NUMBER" not-null="false" />
    </property>
    <property name="CHECK_TIME">
      <column name="CHECK_TIME" sql-type="DATE" not-null="false" />
    </property>
    <property name="CHECK_TYPE">
      <column name="CHECK_TYPE" sql-type="NUMBER" not-null="false" />
    </property>
    <property name="USER_NUM">
      <column name="USER_NUM" sql-type="VARCHAR2" not-null="false" />
    </property>
    <property name="DESCRIPTION">
      <column name="DESCRIPTION" sql-type="VARCHAR2" not-null="false" />
    </property>
    <property name="FIELD1">
      <column name="FIELD1" sql-type="VARCHAR2" not-null="false" />
    </property>
    <property name="FIELD2">
      <column name="FIELD2" sql-type="VARCHAR2" not-null="false" />
    </property>
    <property name="FIELD3">
      <column name="FIELD3" sql-type="VARCHAR2" not-null="false" />
    </property>
  </class>
</hibernate-mapping>