<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="LIMS.Model" namespace="LIMS.Model.EntityModel" xmlns="urn:nhibernate-mapping-2.2">
  <class name="ST_THERMAL_INFO" table="ST_THERMAL_INFO" lazy="true" schema="AD" >
    <id name="ID" column="ID">
      <generator class="sequence">
        <param name="sequence">S_ST_THERMAL_INFO_ID</param>
      </generator>
    </id>
    <property name="THERMAL_STATUS">
      <column name="THERMAL_STATUS" sql-type="NVARCHAR2" not-null="false" />
    </property>
    <property name="MAX_TEMP">
      <column name="MAX_TEMP" sql-type="NUMBER" not-null="false" />
    </property>
    <property name="MIN_TEMP">
      <column name="MIN_TEMP" sql-type="NUMBER" not-null="false" />
    </property>
    <property name="DROP_NUM">
      <column name="DROP_NUM" sql-type="NUMBER" not-null="false" />
    </property>
  </class>
</hibernate-mapping>